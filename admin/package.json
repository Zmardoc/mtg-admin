{
  "name": "mtg-admin",
  "version": "0.0.1",
  "description": "Admin web for commander decks and card collection",
  "productName": "Mtg collector",
  "author": "Martin Mol mol1@seznam.cz",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "deletePublic": "rmdir /s /q \"../api/public\"",
    "copydist": "xcopy \"dist/spa\" \"../api/public\" /s /i /e",
    "buildPublic": "npm run build && npm run deletePublic && npm run copydist",
    "publish": "git add . && git commit -m \"publish\" && git push",
    "check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@tanstack/vue-query": "^4.35.3",
    "axios": "^1.2.1",
    "pinia": "^2.1.6",
    "quasar": "^2.12.7",
    "tesseract.js": "^5.0.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.6.2",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vue-tsc": "^1.8.13"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}

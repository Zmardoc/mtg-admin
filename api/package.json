    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.37.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "swagger-autogen": "^2.23.1",
    "typescript": "^5.0.3"

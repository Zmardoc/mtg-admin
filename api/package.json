  "dependencies": {
    "@types/cors": "^2.8.14",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^5.7.0",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
